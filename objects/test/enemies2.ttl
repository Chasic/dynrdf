@prefix dynrdf: <http://dynrdf.com/objects.rdfs#> .
@prefix def: <http://dynrdf.com/objects#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

def:enemies
    a               dynrdf:Object ;
    dynrdf:type     "TURTLE"^^xsd:string ;
    dynrdf:name     "Enemies2"^^xsd:string ;
    dynrdf:vendor   "Dynrdf"^^xsd:string ;
    dynrdf:regex    "(?<!proxy\\/)enemys"^^xsd:string ;
    dynrdf:priority "1"^^xsd:integer ;
    dynrdf:objectTemplate """# regex za @0 je pro extrahovani zacatku url
                           @base <[  @0,  "^([^/]*\\/[^/]*\\/[^/]*\\/[^/]*\\/[^/]*\\/[^/]*\\/)"]> .
                           @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
                           @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
                           @prefix foaf: <http://xmlns.com/foaf/0.1/> .
                           @prefix rel: <http://www.perceive.net/schemas/relationship/> .


                           <[@1]/[@2]>
                              # konstrukce url - prohozeni parametru
                               rel:enemyOf <[ @2 ]/[ @1 ]> ;
                               a foaf:Person ;
                               foaf:name "[@1]" ."""^^xsd:string ;
    dynrdf:htmlTemplate ""^^xsd:string .

